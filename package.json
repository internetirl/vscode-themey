{
    "name": "themey",
    "displayName": "Themey",
    "description": "Create Themes From Images",
    "version": "1.0.2",
    "publisher": "mattferri",
    "author": {
        "name": "Matt Ferri",
        "email": "mattferri@gmail.com"
    },
    "engines": {
        "vscode": "^1.26.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/internetirl/vscode-themey.git"
    },
    "bugs": {
        "url": "https://github.com/internetirl/vscode-themey/bugs"
    },
    "icon": "icon.png",
    "galleryBanner": {
        "color": "#1e1e1e",
        "theme": "dark"
    },
    "categories": [
        "Themes"
    ],
    "keywords": [
        "theme",
        "customization",
        "tool",
        "generator",
        "custom"
    ],
    "activationEvents": [
        "onCommand:extension.runThemey",
        "onWebviewPanel:themey"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.runThemey",
                "title": "Themey: Create Theme From Image"
            }
        ],
        "themes": [
            {
                "label": "Themey",
                "uiTheme": "vs-dark",
                "path": "./themes/themey.json"
            },
            {
                "label": "Themey Alt",
                "uiTheme": "vs-dark",
                "path": "./themes/themey-alt.json"
            },
            {
                "label": "Themey 16 Colors",
                "uiTheme": "vs-dark",
                "path": "./themes/themey-16colors.json"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "dependencies": {
        "mustache": "^2.3.2",
        "node-vibrant": "^3.0.0",
        "strip-json-comments": "^2.0.1",
        "vscode-theme-generator": "*"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "tslint": "^5.8.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    }
}
