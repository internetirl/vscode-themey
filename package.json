{
    "name": "themey",
    "displayName": "Themey",
    "description": "Create Themes From Images",
    "version": "0.0.1",
    "publisher": "mattferri",
    "engines": {
        "vscode": "^1.26.0"
    },
    "icon": "logo.png",
    "categories": [
        "Themes"
    ],
    "activationEvents": [
        "onCommand:extension.runThemey"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.runThemey",
                "title": "Themey: Create Theme From Image"
            }
        ],
        "themes": [
            {
                "label": "Themey",
                "uiTheme": "vs-dark",
                "path": "./themes/themey.json"
            }       
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "tslint": "^5.8.0",
        "node-vibrant": "^3.0.0",
        "vscode-theme-generator": "*",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    }
}
